{
  "name": "lurkdown",
  "version": "0.0.1",
  "description": "A simple transpiler that takes markdown files and then transpiles to styled html files.",
  "main": "./bin/index.js",
  "bin": {
    "lurkdown": "./bin/index.js",
    "obsfx@lurkdown": "./bin/index.js"
  },
  "scripts": {
    "run": "ts-node ./src/index.ts",
    "test-html": "ts-node ./src/index.ts > ./index.html",
    "inspect": "node --inspect -r ts-node/register ./src/index.ts",
    "build": "tsc -b",
    "watch": "tsc -b --watch"
  },
  "keywords": [
    "markdown",
    "commandline",
    "tool",
    "transpiler",
    "html"
  ],
  "author": "obsfx",
  "license": "WTFPL",
  "devDependencies": {
    "@types/node": "^14.0.27",
    "@types/uuid": "^8.3.0",
    "ts-node": "^9.0.0",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@types/highlight.js": "^9.12.4",
    "highlight.js": "^10.2.0",
    "uuid": "^8.3.0"
  }
}
